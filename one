int main(){
    int a[100][100];
    int X, N;
    std::cin >> X >> N;
    for(int i = 0; i < N; i++){
        for(int j = 0; j < N; j++){
            std::cin >> a[i][j];
        }
    }
    for(int i = 0; i < N; i++){
        bool ans = false;
        for(int j = 0; j < N; j++){
            if(a[j][i] == X){
                ans = true;
                break;
            }
        }
        std::cout << (ans?"YES":"NO") << std::endl;
    }
    return 0;
}
N, K = map(int, input().split())
st = list(map(int, input().split()))
l = 0
r = st[len(st) - 1] - st[0]

while(l < r - 1):
    m = (l + r) // 2
    cows = 1
    i = 0
    while(i < len(st) - 1):
        for j in range(i + 1, len(st)):
            #print(st[i], st[j])
            if(st[j] - st[i] >= m):
                i = j - 1
                cows += 1
                break
        i += 1
    print(cows, l, r, m)
    if(cows < K):
        r = m
    else:
        l = m
