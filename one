#include <iostream>
#include <cmath>

int sq(int a){
    int i = 0;
    while(i * i * i <= a){
        i++;
    }
    return i - 1;
}

int main(){
    int n;
    std::cin >> n;
    int a[n + 1] = {0};
    a[1] = 1;
    for(int i = 1; i < n + 1; i++){
        a[i] = n + 2;
        int k = sq(i);
        for(int j = 1; j <= k; j++){
            //std::cout << a[i] << " " << a[i - int(pow(j, 3))] << std::endl;
            int g = j * j * j;
            if(a[i] > a[i - g] + 1){
                a[i] = a[i - g] + 1;
            }
        }
    }
    std::cout << a[n] << std::endl;
    return 0;
}
n = int(input())

a = [0] * (n + 1)
for i in range(n + 1):
    a[i] = [0] * (int(n ** (1. / 3.)) + 1)

def F(n):
    print(n)
    if(n == 0):
        return 0
    if(n < 0):
        return float('inf')
    ret = float('inf')
    for i in range(1, int(n ** (1. / 3.)) + 1):
        a = F(n - i ** 3)
        if(ret > a):
            ret = a
    return ret + 1


print(F(n))
